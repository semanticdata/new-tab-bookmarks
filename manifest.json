{
  "manifest_version": 2,
  "name": "New Tab Bookmarks",
  "version": "1.3.1",
  "description": "New tab filled with your own bookmarks and a place to write notes.",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "icons": {
    "48": "src/icons/icon48.png",
    "96": "src/icons/icon96.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "newtab-bookmarks@semanticdata",
      "strict_min_version": "100.0"
    }
  },
  "chrome_url_overrides": {
    "newtab": "src/index.html"
  }
}