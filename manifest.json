{
  "manifest_version": 2,
  "name": "New Tab Bookmarks",
  "version": "1.1.0",
  "description": "New tab filled with your bookmarks.",
  "permissions": ["bookmarks", "storage"],
  "icons": {
    "48": "src/icon.svg",
    "96": "src/icon.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "newtab-bookmarks@semanticdata",
      "strict_min_version": "100.0"
    }
  },
  "chrome_url_overrides": {
    "newtab": "src/index.html"
  }
}
